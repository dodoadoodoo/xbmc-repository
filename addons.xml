<addon id="repository.dodoadoodoo.xbmc-plugins" name="dodoadoodoo Add-on Repository" version="1.0.0" provider-name="dodoadoodoo">
        <extension point="xbmc.addon.repository" name="dodoadoodoo Add-on Repository">
                <info compressed="false">https://raw.github.com/dodoadoodoo/xbmc-repository/master/addons.xml</info>
                <checksum>https://raw.github.com/dodoadoodoo/xbmc-repository/master/addons.xml.md5</checksum>
                <datadir zip="true">https://raw.github.com/dodoadoodoo/xbmc-repository/master/</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
                <summary>Install Add-ons from dodoadoodoos Add-on Repository</summary>
                <description>Video plug-ins for XBMC</description>
                <disclaimer></disclaimer>
                <platform>all</platform>
        </extension>
</addon>
</addons>
