<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.24hd"
       name="24HD"
       version="0.0.1"
       provider-name="Daniel, Popeye">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>24HD</summary>
    <description>Watch 24HD</description>
    <description lang="sv">Titta på 24HD</description>
    <disclaimer></disclaimer>
  </extension>
</addon>

<addon id="plugin.video.expressentv"
       name="Expressen TV"
       version="0.0.3"
       provider-name="Daniel, Popeye">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
